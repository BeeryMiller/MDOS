WRTFCB MOV  R9,@RAMBUF
       MOV  @FNLOCF(R3),@AUNUM
       MOVB @FNSEC0(R3),R0
       SRL  R0,12
       MOV  R0,@SCINAU
       BLWP @WSECTR          STATUS IS IN THE = BIT ON RETURN
TOPFC1 RT
*
TOPFC4 MOV  R12,R12           THIS IS THE CRU BASE, SO EQ IS RESET
TOPFC5 RT
*
TOPFCB MOV  R9,@RAMBUF
TOPFC2 MOV  @PTRPRE(R9),R0
       JEQ  TOPFC4            FIX THIS TO BE <>
       MOV  R0,@AUNUM
       CLR  @SCINAU
TOPFC3 BLWP @RSECTR
       JEQ  TOPFC1            ERROR IN READ
       MOV  @AUNUM,@FNLOCF(R3)
       SZCB @CBHF0,@FNSEC0(R3)  ZERO OUT SCINAU FOR FDR
       JMP  TOPFC2
